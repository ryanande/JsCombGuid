{
  "name": "jscombguid",
  "version": "1.0.8",
  "description": "JavaScript module used to create comb guids.",
  "main": "lib/index.js",
  "types": "lib/index",
  "scripts": {
    "clean": "rimraf lib",
    "test": "nyc mocha --require babel-core/register",
    "lint": "eslint src/**",
    "build": "npm run clean && npm run lint && npm run test && babel ./src -d ./lib",
    "upgrade-interactive": "npm-check --update",
    "prepublish": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanande/JsCombGuid.git"
  },
  "keywords": [
    "guid",
    "combguid",
    "guid generator"
  ],
  "author": "Ryan Anderson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanande/JsCombGuid/issues"
  },
  "homepage": "http://ryanande.github.io/JsCombGuid/",
  "dependencies": {
    "moment": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.6.0",
    "mocha": "^6.1.4",
    "npm-check": "^5.9.0",
    "nyc": "^14.0.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
